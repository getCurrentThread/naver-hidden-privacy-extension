{
  "manifest_version": 3,
  "name": "프라이버시 숨기기",
  "description": "사이트 내에 개인정보를 숨깁니다",
  "version": "0.1.2.0",
  "author": "getCurrentThread",
  "action": {
    "default_icon": "icons/icon_on_256.png"
  },
  "icons": {
    "16": "icons/icon_off_16.png",
    "48": "icons/icon_off_48.png",
    "128": "icons/icon_off_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "css": ["content.css"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
